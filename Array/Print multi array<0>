#include<bits/stdc++.h>
using namespace std;
int main()
{
    int count = 0;
    int array[3][4];
    for (int i = 0; i < 3; i++) 
    {
        for(int j = 0; j < 4; j++)
        {
            array[i][j] = count;
            count+=2;
        }
    }
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            cout << array[i][j] << " ";
        }
        cout <<"\n";
    }
    return 0;
}
