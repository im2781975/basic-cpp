// swap()
#include<iostream>
#include<array> 
using namespace std;

void print(array<int, 6> arr) 
{
    for(int i=0; i<6; i++)
    {
        cout << arr[i] << " ";
    }
}
int main()
{
    array<int,6> ar = {1, 2, 3, 4, 5, 6};
    
    array<int,6> ar1 = {7, 8, 9, 10, 11, 12};
 
    cout << "\nFirst array elements before swapping are : ";
    print(ar);

    cout << "\nSecond array elements before swapping are : ";
    print(ar1);
    
    ar.swap(ar1);
 
    cout << "\nFirst array elements after swapping are : ";
    print(ar);
    
    cout << "\nSecond array elements after swapping are : ";
    print(ar1);
 
    return 0;
}
