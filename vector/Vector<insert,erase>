#include <iostream>
#include <vector>
using namespace std;
int main()
{
    vector<int> v = { 1, 2, 3 };
 
    vector<int>::iterator i;
    int j;
 
    for (i = v.begin(); i != v.end(); ++i) {
        if (i == v.begin()) {
            i = v.insert(i, 5);
            
        }
    }
    cout<<"After insertion: ";
    for (i = v.begin(); i != v.end(); ++i) {
        cout <<*i << " ";
    }
    cout<<"\n";
//---------------------------------

    for (i = v.begin(); i != v.end(); ++i) {
        if (i == v.begin() + 1) {
            i = v.erase(i);
        }
    }
    cout<<"After deletion: ";
    for (i = v.begin(); i != v.end(); ++i) {
        cout <<*i << " ";
    }
 
    return 0;
}
