#include <iostream>
#include <vector>
using namespace std;
int main()
{
    vector<int> v{ 1, 2, 3 };
 
    vector<int>::iterator i;
 
    cout << "Without iterators = ";
    for (int j = 0; j < 3; ++j) {
        cout << v[j] << " ";
    }
    
    cout << "\nWith iterators = ";
    for (i = v.begin(); i != v.end(); ++i) {
        cout << *i << " ";
    }
 
    v.push_back(4);
 
    cout << "\nWithout iterators = ";
    for (int j = 0; j < 4; ++j) {
        cout << v[j] << " ";
    }
 
    cout << "\nWith iterators = ";
    for (i = v.begin(); i != v.end(); ++i) {
        cout << *i << " ";
    }
    return 0;
}
