//distance used to find count elements.
#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>v {2,3,4,8,5,7,8 };
    
    vector<int> ::iterator i1;
    vector<int> ::iterator i2;
    
    i1=v.begin();i2=v.end();
    if(i1!=i2)
    {
        cout<<"Yes" << "\n";
    }
    
    int cnt=distance(i1,i2);
    cout<<cnt;
}
