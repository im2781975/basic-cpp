// C++ program to demonstrate Random Access iterator
#include <iostream>
#include <vector>
using namespace std;
int main()
{
    vector<int> v1 = { 1, 2, 3, 4, 5 };
    int i;
 
    // Accessing elements using offset dereference
    // operator [ ]
    for (i = 0; i < 5; ++i) {
        cout << v1[i] << " ";
    }
    return 0;
}
