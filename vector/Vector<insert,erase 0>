#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int>a;
    for(int i=0;i<n;i++)
    {
        a.push_back(i);
    }
    a.insert(a.begin()+1,100);
    for(int i=0;i<a.size();i++)
    {
        cout<<" "<<a[i];
    }
    cout<<endl;
    cout<<"after delete:";
    a.erase(a.begin()+2);
    //for deleting last element use 
    for(int i=0;i<a.size();i++)
    {
        cout<<" "<<a[i];
    }
    cout<<endl;
    vector<int>b={1,2,3,4,5};
    b.resize(3);
    for(int i=0;i<b.size();i++)
    {
        cout<<" "<<b[i];
    }
    
}
