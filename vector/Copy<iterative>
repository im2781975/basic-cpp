//copy of vectorby iterative method
#include<iostream>  
#include<vector>  
using namespace std;  
  
int main()  
{  
    vector<int> vect1{1, 2, 3, 4}; 
    
    vector<int> vect2;  
  
    vect2 = vect1;  
  
    cout<<"Before copy vector : ";  
    for (int i=0; i<vect1.size(); i++)  
        cout << vect1[i] << " ";  
  
    cout << "\nAfter copy vector is: ";  
    for (int i=0; i<vect2.size(); i++)  
        cout << vect2[i] << " ";  
 
    vect1[0] = 2;  
  
    cout << "\nFirst element of vect1[] is :"<<vect1[0];

    cout << "\nFirst element of vect2[] is :"<<vect2[0];
  
    return 0;  
}  
