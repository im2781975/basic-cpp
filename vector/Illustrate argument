#include<bits/stdc++.h>
using namespace std;
int main()
{
    //create a obj without specifying type
    vector v1 {2.3, 8.7, 4.5, 9.7};
    cout<<"Elements are(float): ";
    for(auto i: v1)
        cout<< i << " ";
        
    cout<< "\n";
    
    vector v2 {2, 6, 9, 7, 6, 9};
    cout<<"Elements are(int): ";
    for(auto i: v2 )
        cout<< i <<" ";
        
    cout<< "\n";
}
