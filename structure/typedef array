#include <iostream>
using namespace std;
int main()
{
    typedef int arr[3];
    arr array1{1 , 1, 1};
    cout << "Array output: "
         << "\n";
    for (int i = 0; i < 3; i++) {
        cout << array1[i] << " ";
    }
    cout << "\n";
    
    arr matrix[3];
    cout << "Matrix output: "
         << "\n";
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            matrix[i][j] = i * j;
            }
    }
  
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << matrix[i][j] << "  ";
        }
        cout << "\n";
    }
    return 0;
}
