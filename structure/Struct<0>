#include <bits/stdc++.h>
using namespace std;
struct GFG {
    int G1;
    char G2;
    float G3;
};
int main()
{
    struct GFG Geek;
    Geek.G1 = 85;
    Geek.G2 = 'G';
    Geek.G3 = 989.45;
    cout<<"The value is : "<<Geek.G1 <<"\n";
    cout << "The value is : " << Geek.G2 <<"\n";
    cout << "The value is : " << Geek.G3 <<"\n";
    return 0;
}
