void intro_6()
{
//dynamic allocation
    int* ptr=new int[10]{2,3,6,8,9};
    for(int i=0;i<10;i++)
    {
        cout<<ptr[i]<<" ";
    }
    delete ptr;
}
void intro_7()
{//swap:
    int n[]={2,4,1,5,6},x=5,tmp;
    for(i=0;i<x-1;i++)
    {
        for(j=i+1;j<x;j++)
        {
            if(n[i]>n[j])
            {
                tmp=n[i];
                n[i]=n[j];
                n[j]=tmp;
            }
        }
    }
    cout<<n[i];
}
void intro_8()
{//minimum
    int ar[5]={2,1,3,6,9};
    //sort(a,a+2);
    int mn=ar[0];
    for(i=0;i<5;i++)
    {
        int mn=min (ar[i],mn);
    }
    cout<<mn;
}
void intro_9()
{
    int n=6;
    string st;
    st.resize(n);
    //string st(10,'z');
    cout<<st.capacity()<<" ";
    cout<<st.max_size()<<" ";
    cout<<st.length()<<" ";
    cout<<st.size()<<" ";
}
void intro_10()
{
    string st="abcdefghij";
    string::reverse_iterator it;
//for(auto it=st.rbegin();it!=st.rend();it++)
    for(it=st.rbegin();it!=st.rend();it++)
    {
        cout<<*it;
    }
}
void intro_11()
{
    string st="abcdefghij";
    st.clear();
    if(st.empty())
    cout<<"Yes";
}
