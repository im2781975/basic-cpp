#include <iostream>
using namespace std;

namespace first_space
{
   void func()
   {
      cout << "first_space" << endl;
   }
   namespace second_space
   {
      void func()
      {
         cout << "second_space" << endl;
      }
   }
}
using namespace first_space::second_space;
int main ()
{
   func();
   return 0;
}
