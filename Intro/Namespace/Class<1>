#include<bits/stdc++.h>
using namespace std;

namespace first
{
    void display();
    class base
    {
        public:
        void display();
    };
}
void first::base::display()
{
    cout<<"abcd";
}
void first::display()
{
    cout<<"efgh";
}
int main()
{
    first::base b;
    first::display();
    b.display();
}

