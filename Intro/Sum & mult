#include<bits/stdc++.h>
using namespace std;

int i,n,sum_digit=0,mult_digit=1;
int sum(int arr[],int n)
{
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        sum_digit+=arr[i];
    }
    cout<<sum_digit<<endl;
}
int mult(int arr[],int n)
{
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        if(i%2==0)
        {
        mult_digit*=arr[i];
        }
    }
    cout<<mult_digit<<endl;
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int arr[n+1];
    cin>>n;
    //sum(arr,n);
    mult(arr,n);
}
