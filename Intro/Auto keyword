// working of auto keyword 
#include <bits/stdc++.h> 
using namespace std; 
 
int main() 
{ 
    auto an_int = 26; 
    auto a_bool = false; 
    auto a_float = 26.2; 
    auto ptr = &a_float; 
  
  //return a type of every data type
    cout <<typeid(a_bool).name()<< "\n"<< typeid(an_int).name() << "\n"<< typeid(a_float).name() << "\n"; 
    
    return 0; 
}
