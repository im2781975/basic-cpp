#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int mini=arr[0];
    int maxi=arr[0];
    int sum=0;
    for(int i=0;i<n;i++)
    {
        mini=min(mini,arr[i]);
        maxi=max(maxi,arr[i]);
        sum+=arr[i];
    }
    cout<<maxi<<"\n"<<mini<<"\n"<<sum<<"\n";
    
}
