#include <iostream>
using namespace std;
class Test {
    int a;
  
public:
    Test() { a = 1; }
  
    // Local parameter 'a' hides object's member
    // 'a', but we can access it using this.
    void func(int a)
    { //cout << this->a; 
      //cout<<a;
      cout<<Test::a;
}
};
  
// Driver code
int main()
{
    Test obj;
    int k = 3;
    obj.func(k);
    return 0;
}
