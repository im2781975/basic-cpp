#include <iostream>
#include <set>

int main() {
    std::set<int> set1, set2, unionSet;

    // Insert elements into the first set
    set1.insert(1);
    set1.insert(2);
    set1.insert(3);

    // Insert elements into the second set
    set2.insert(2);
    set2.insert(3);
    set2.insert(4);

    // Find the union of the two sets
    for (int element : set1) {
        unionSet.insert(element);
    }
    for (int element : set2) {
        unionSet.insert(element);
    }

    // Print the elements in the union set
    for (int element : unionSet) {
        std::cout << element << " ";
    }

    return 0;
}

