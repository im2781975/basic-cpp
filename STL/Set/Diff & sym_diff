// set_difference() and set_symmetric_difference()
#include<iostream>
#include<algorithm> 
#include<vector> 
using namespace std;
int main()
{
    vector<int>v1{1,3,4,5,20,30};
     
    vector<int>v2{1,5,6,7,25,30};
     
    vector<int> v3(10);
     
    vector<int> v4(10);
     
     
    //using set_difference() to compute difference of 2 containers v1 and v2.
 auto it = set_difference(v1.begin(), v1.end(), v2.begin(), v2.end(), v3.begin());
     
 // using set_symmetric_difference() to compute symmetric_difference of 2 containers
 auto it1 =set_symmetric_difference(v1.begin(), v1.end(), v2.begin(), v2.end(), v4.begin());
     
    v3.resize(it - v3.begin());
    v4.resize(it1 - v4.begin());
    
    cout << "\nDifference of two containers is : ";
    for (int &x : v3) 
     cout << x << " ";
     
    cout << "\nsymmetric_difference of two containers is : ";
    for (int &x : v4) 
     cout << x << " ";
    return 0;    
}
