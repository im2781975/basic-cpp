#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector v1 {2.3, 8.7, 4.5, 9.7};
    cout<<"Elements are(float): ";
    for(auto i: v1)
        cout<< i << " ";

    vector v2 {2, 6, 9, 7, 6, 9};
    cout<<"\nElements are(int): ";
    for(auto i: v2 )
        cout<< i <<" ";
        
    vector<string> value{ "This","is","foreach","example", "using", "vector." };
 
    cout<<"\nElements are(string) : ";
    for (auto v : value)
        cout << v << " ";
 
}
