// application of make_heap() (max_heap) 
// priority scheduling 
#include<iostream> 
#include<algorithm>
#include<vector> 
using namespace std; 
  
int main() 
{ 
    vector<int>vi{15,6,7,9,11,19}; 

    int k = 3; 

    make_heap(vi.begin(),vi.end()); 
    int a = 10; 
      
    for ( int i=0; i<k; i++) 
    { 
        vi.push_back(a); 
      
    // transform into heap ( using push_heap() ) 
    push_heap(vi.begin(), vi.end()); 
      
    cout <<"\nJob with maximum priority is :"<<vi.front();
    a = a + 10; 
    } 
} 
