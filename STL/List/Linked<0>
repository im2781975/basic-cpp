#include <iostream>
#include <list>

std::list<int> linkedList; // Assuming you have a linked list defined

int getValue(int index) {
    int count = 0;

    // Iterate through the list until the specified index
    for (const int &value : linkedList) {
        if (count == index) {
            return value;
        }
        count++;
    }

    // If the index is out of range, return -1
    return -1;
}

int main() {
    // Populate your linked list with values
    linkedList = {3, 2, 6, 4, 5};
    
    int index = 2;
    int result = getValue(index);
    
    if (result != -1) {
        std::cout << "Sample Output: " << result << std::endl;
    } else {
        std::cout << "Index is out of range." << std::endl;
    }
    
    return 0;
}
