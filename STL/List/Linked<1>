#include <iostream>
#include <list>

bool search(const std::list<int>& linkedList, int value) {
    for (const int& element : linkedList) {
        if (element == value) {
            return true;
        }
    }
    return false;
}

int main() {
    std::list<int> linkedList = {3, 2, 6, 4, 5};
    int valueToSearch = 4;

    if (search(linkedList, valueToSearch)) {
        std::cout << "true" << std::endl;
    } else {
        std::cout << "false" << std::endl;
    }

    return 0;
}

