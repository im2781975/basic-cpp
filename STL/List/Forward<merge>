//working of merge() and operator= 
#include<iostream> 
#include<forward_list> 
using namespace std; 
 
int main() 
{     
    forward_list<int>flist1{1,2,3};
    
    forward_list<int> flist2; 
     
    // Creating copy using "="
    flist2 = flist1; 
     
    // Displaying flist2 
    cout<<"Contents of 2nd forward list after copy are : "; 
    for (int &x : flist2) 
        cout << x << " "; 
     
    flist1.merge(flist2); 
    
    cout << "\nContents of forward list after merge are : "; 
    for (int &x : flist1) 
        cout << x << " "; 
     
    return 0;     
} 
