#include <iostream>
#include <list>
using namespace std;
int main()
{
    list<int>mylist{1, 2, 3, 4, 5};
 
    cout << "List is: ";
    for (auto it = mylist.begin(); it != mylist.end();it++) {
        cout << *it << " ";
    }
 
    mylist.reverse();
 
    cout << "\nReversed list: ";
    for (auto it = mylist.begin(); it != mylist.end();it++) {
        cout << *it << " ";
    }
    return 0;
}
