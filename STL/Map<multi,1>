#include <iostream>
#include <map>
using namespace std;
int main()
{
    map<string, int> map;

    map["one"] = 1;
    map["two"] = 2;
    map["three"] = 3;
 
    cout << "Key: one, Value: " << map["one"] <<"\n";
    cout << "Key: two, Value: " << map["two"] << "\n";
    cout << "Key: three, Value: " << map["three"] <<"\n";
   
  if (map.count("four") > 0)
  {
        cout << "Key 'four' is in the map" << "\n";
  }
  else
  {
        cout << "Key 'four' is not in the map" << "\n" ;
  }
  return 0;
}
