// demonstrate Initialization, indexing, and iteration 
#include <iostream> 
#include <unordered_map> 
using namespace std; 
 
int main() 
{ 
  unordered_map<string, double> umap = {{"One", 1},{"Two", 2}, {"Three", 3} }; 
  //inserting values by using [] operator 
  umap["PI"] = 3.14; 
  umap["root2"] = 1.414; 
  umap["root3"] = 1.732; 
  umap["log10"] = 2.302; 
  umap["loge"] = 1.0; 
  
  //inserting value by insert func
  umap.insert(make_pair("e",2.718)); 

  string key = "PI"; 
  if (umap.find(key) == umap.end())
    cout << key << "not found"; 
  else
    cout << "Found " << key <<" ";
    
  unordered_map<string, double>::iterator itr; 
  cout << "\nElements are: "; 
  for (itr = umap.begin();  
       itr != umap.end(); itr++)  
  { 
    //itr->first stores key & itr->second stores value.
    cout << itr->first << "->" <<  
            itr->second << "\n"; 
  } 
}
