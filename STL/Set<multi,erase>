#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    multiset<int> a;
    a.insert(10);
    a.insert(10);
    a.insert(10);
    cout << a.count(10) << "\n";
 
    // it will remove only one value of
    a.erase(a.find(10));
    cout << a.count(10)<<"\n";
 
    // it will remove all instance of value 10
    a.erase(10);
    cout << a.count(10)<<"\n";
 
    return 0;
}
