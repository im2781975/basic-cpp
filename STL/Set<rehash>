// unordered_set::rehash()
#include <iostream>
#include <string>
#include <unordered_set>
 
using namespace std;
int main()
{
    unordered_set<string> us;
    us.rehash(9);

    us.insert("geeks");
    us.insert("for");
    us.insert("geeks");
    us.insert("users");
 
    for (auto it = us.begin(); it != us.end(); it++) {
        cout << *it << " ";

    }
    cout <<"\nThe bucket count is "
         << us.bucket_count();
    return 0;
}
