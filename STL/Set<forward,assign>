//forward list & assign()
#include <forward_list>
#include <iostream>
using namespace std;
 
int main()
{
    forward_list<int> list1;
    list1.assign({ 1, 2, 3 });
    
    forward_list<int> list2;
    list2.assign(5, 10);
    
    forward_list<int> list3;
    list3.assign(list1.begin(), list1.end());
    
    
    cout << "The elements of first forward list are : ";
    for (int& a : list1)
        cout << a << " ";
    cout << "\n";
 
    cout << "The elements of second forward list are : ";
    for (int& b : list2)
        cout << b << " ";
    cout << "\n";

       
      cout << "elements of third forward list are :";
    for (int& c : list3)
        cout << c << " ";
    cout << "\n";
 
    return 0;
}
