// working of minmax()
#include<iostream>
#include<algorithm>
using namespace std;
 
int main()
{
    pair<int, int> mnmx;
 
    mnmx = minmax(53, 23);
    
    cout << "Minimum value obtained is : "<<mnmx.first;
    
    cout<<"\nMaximum value obtained is :"<<mnmx.second;
     
    // Using minmax((array of elements)
    mnmx = minmax({2, 5, 1, 6, 3});

    cout << "\nMinimum value is[array]: "<< mnmx.first;

    cout << "\nMaximum value is[array] : "<<mnmx.second;
}
