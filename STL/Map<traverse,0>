#include <iostream>
#include <map>
using namespace std;

int main()
{
    map<string, int> myMap;

    myMap["one"] = 1;
    myMap["two"] = 2;
    myMap["three"] = 3;

    cout << "Key: one, Value: " << myMap["one"] << "\n";
    cout << "Key: two, Value: " << myMap["two"] << "\n";
    cout << "Key: three, Value: " << myMap["three"] << "\n";

    if (myMap.count("four") > 0)
    {
        cout << "Key 'four' is in the map" << "\n";
    }
    else
    {
        cout << "Key 'four' is not in the map" << "\n";
    }
    map<string, int>::iterator it;
    it = myMap.begin();

    // Iterate through the map and print the elements
    while (it != myMap.end())
    {
        cout << "Key: " << it->first << ", Value: " << it->second << "\n";
        ++it;
    }
    return 0;
}

