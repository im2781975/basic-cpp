#include <bits/stdc++.h>
using namespace std;

typedef unordered_multimap<string, int>::iterator unit;

void printUmm(unordered_multimap<string, int> umm) {
    unit it = umm.begin();
 
    for (; it != umm.end(); it++) {
        cout << "<" << it->first << ", " << it->second << ">  ";
    }
}

int main() {
    unordered_multimap<string, int> umm1;
    
    unordered_multimap<string, int> umm2 = {
        { "apple", 1 }, { "ball", 2 },
        { "apple", 10 }, { "cat", 7 },
        { "dog", 9 }, { "cat", 6 },
        { "apple", 1 }
    };
 
    // Initialization by assignment operation
    umm1 = umm2;
    printUmm(umm1);
 
    if (umm2.empty())
        cout << "unordered multimap 2 is empty";
    else
        cout << "unordered multimap 2 is not empty";
 
    cout << "\nSize of unordered multimap 1 is "
         << umm1.size() ;
 
    string key = "apple";
    unit it = umm1.find(key);
    if (it != umm1.end()) {
        cout << "\nkey '" << key << "' is there in unordered multimap 1";
    } else {
        cout << "\nkey '" << key << "' isn't there in unordered multimap 1";
    }
 
    // count returns the count of the total number of pairs
    int cnt = umm1.count(key);
    cout << "\ntotal values associated with '" << key
         << "' are " << cnt ;
 
    printUmm(umm2);
    umm2.insert(make_pair("dog", 11));
    
    umm2.insert({ { "alpha", 12 }, { "beta", 33 } });
    cout << "\nAfter insertion :";
    printUmm(umm2);
 
    umm2.erase("apple");
    cout << "\nAfter deletion of apple";
    printUmm(umm2);

    umm1.clear();
    umm2.clear();
 
    if (umm2.empty())
        cout << "\nunordered multimap 2 is empty";
    else
        cout << "\nunordered multimap 2 is not empty";
}
