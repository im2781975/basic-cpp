// the use of shuffle 
#include <bits/stdc++.h> 
using namespace std; 
  
int main() 
{ 
    array<int,5>s{ 1, 2, 3, 4, 5 }; 
    unsigned seed = 0; 
  
    shuffle(s.begin(), s.end(), default_random_engine(seed)); 
  
    cout<<"shuffled elements are:"; 
    for (int& i : s) 
        cout << ' ' << i; 
    return 0; 
} 
