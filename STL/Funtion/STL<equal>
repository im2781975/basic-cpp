// use of  bool equal
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int v[]{10, 20, 30, 40, 50};
    vector<int> vect(v, v+sizeof(v) / sizeof(int) );
 
    cout <<"Elements of vect is: ";
    for (unsigned int i = 0; i < vect.size(); i++)
        cout << " " << vect[i];
 
    // using std::equal()
    // Comparison within default constructor
    if (equal (vect.begin(), vect.end(), v) )
        cout << "\nContents of both sequences are equal";
    else
        printf("\nContents of both sequences differ.");
 
}
