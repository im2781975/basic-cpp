#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<int> v{2, 3, 4, 5, 6, 7, 9};
    vector<int> m{4, 5, 6};

    for (vector<int>::iterator it = v.begin(); it != v.end(); ++it) {
        vector<int>::iterator m_it = m.begin();
        vector<int>::iterator temp_it = it;

        while (m_it != m.end() && *temp_it == *m_it) {
            ++temp_it;
            ++m_it;
        }

        if (m_it == m.end()) {
            cout << it - v.begin();
            return 0; // Found the subsequence, exit the loop
        }
    }
    cout << "Subsequence not found in the vector.";
    return 0;
}

