// rotate vector using rotate algorithm
#include<bits/stdc++.h>
using namespace std;
 
int main () {
    vector<int>vec1{1,2,3,4,5,6,7};
 
    cout <<"Vector is : ";
    for(int i=0; i < vec1.size(); i++)
        cout << " " << vec1[i];

    // Rotate vector left 3 times.
    int rotL=3;
 
    rotate(vec1.begin(), vec1.begin()+rotL, vec1.end());
 
    cout <<"\nVector after rotation :";
    for (int i=0; i < vec1.size(); i++)
        cout<<" "<<vec1[i];
 
    vector <int> vec2{1,2,3,4,5,6,7,8,9};
 
    cout <<"\nVect2[] is :";
    for (int i=0; i < vec2.size(); i++)
        cout << " " << vec2[i];
 
    // Rotate vector right 4 times.
    int rotR = 4;
 
    rotate(vec2.begin(), vec2.begin()+vec2.size()-rotR, vec2.end());
 
    cout << "\nNew vector after right rotation :";
    for (int i=0; i < vec2.size(); i++)
        cout << " " << vec2[i];
 
return 0;
}
