// std::min_element 
#include <iostream> 
#include <algorithm> 
using namespace std; 
 
bool comp(int a, int b) 
{ 
    return (a < b); 
} 
  
int main() 
{ 
    int v[] = { 9, 4, 7, 2, 5, 10, 11, 12, 1, 3, 6 }; 
  
    int* i1; 
    i1 = min_element(v + 2, v + 9, comp); 
  
    cout << *i1 << "\n"; 
    return 0; 
} 
