// std::iter_swap 
#include <iostream> 
#include <algorithm> 
#include <vector> 
using namespace std; 
int main() 
{ 
    vector<int> v1; 
    int i; 
  
    for (i = 0; i < 10; ++i) 
        v1.push_back(i); 
    
    vector<int>::iterator i1, i2; 
  
    i1 = v1.begin(); 
    i2 = v1.end() - 1; 
    
    cout<<"After swap : ";
    iter_swap(i1, i2);
    for (i = 0; i < 10; ++i) 
        cout << v1[i] << " "; 
        
    vector<int> v2(5, 100); 
      
    swap_ranges(v1.begin() + 3, v1.begin() + 7, v2.begin()); 
    cout<<"\nAfter swap v1[] is: ";
    for (i = 0; i < 10; ++i)  
        cout << v1[i] << " "; 

    cout<<"\nv2[] is: ";
    for (i = 0; i < 5; ++i)  
        cout << v2[i] << " "; 
  
    return 0; 
} 
