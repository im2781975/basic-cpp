#include<bits/stdc++.h>
using namespace std;

int main () 
{
    vector<int> vect(8, 1); 
    cout<<"Elements of vect1[] is: ";
    for (int i=0; i<vect.size(); i++)
        cout << vect[i] << " ";

    //fill()
    vector<int> vect2(8); 
    fill(vect2.begin() + 2, vect2.end() - 1, 4);
    
    cout<<"\nElements of vect2[] is: ";
    for (int i=0; i<vect2.size(); i++)
        cout << vect2[i] << " ";

// Filling the complete vector with value 10 
    fill(vect.begin(), vect.end(), 10); 
    cout<<"\nAfter filling vector[] is: ";
    for (int i =0; i < vect.size(); i++) 
        cout << vect[i] << " "; 

    return 0;
}

