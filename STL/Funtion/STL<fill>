#include<bits/stdc++.h>
using namespace std;

int main () 
{
    vector<int> vect(8, 1); 

    for (int i=0; i<vect.size(); i++)
    { 
        cout << vect[i] << " ";
    }
    cout << "\n";

    vector<int> vect2(8); // You need to declare a new vector here

    fill(vect2.begin() + 2, vect2.end() - 1, 4);

    for (int i=0; i<vect2.size(); i++)
    {
        cout << vect2[i] << " ";
    }
    cout << "\n";

    return 0;
}

