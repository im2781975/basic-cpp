#include <iostream>
#include <deque>
#include <algorithm>

int main() {
    std::deque<int> myDeque = {10, 5, 20, 15, 30};

    // Find the maximum element in the deque
    int maxElement = *std::max_element(myDeque.begin(), myDeque.end());

    // Find the minimum element in the deque
    int minElement = *std::min_element(myDeque.begin(), myDeque.end());

    std::cout << "Maximum element: " << maxElement << std::endl;
    std::cout << "Minimum element: " << minElement << std::endl;

    return 0;
}

