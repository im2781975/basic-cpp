4 8 2
0x11ab2c0
 4 8 2
0x11ab2c0 */
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int *x=new int[n];
    for(int i=0;i<n;i++)
    {
        cin>>x[i];
    }
    cout<<x<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<" "<<x[i];
    }
    cout<<endl;
    cout<<x<<endl;
    delete [] x;
}
