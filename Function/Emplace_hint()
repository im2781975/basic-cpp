//unordered_set::emplace_hint() function 
#include <iostream> 
#include <unordered_set> 

using namespace std;
int main() 
{ 
    unordered_set<int>uset = { 20, 40, 50, 60 }; 
    
    uset.emplace_hint(uset.begin(), 60); 

    cout << "uset: "; 
    for (auto it = uset.begin(); it != uset.end(); it++) 
        cout << *it << " "; 
} 
