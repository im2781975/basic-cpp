// C++ program to demonstrate template
#include <iostream>
using namespace std;
  
// Generic function to find minimum
// of 2 data types
template <typename T>
T Min(T x, T y)
{
    return (x < y) ? x : y;
}
  
// Driver Code
int main()
{
    cout << Min(7, 3) << endl;
    cout << Min('z', 'a') << endl;
  
    return 0;
}
