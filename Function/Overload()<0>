#include <iostream>
using namespace std;

void print(int i)
{
    cout <<" Here is int " << i<<"\n";
}
void print(double f)
{
    cout << " Here is float " << f << "\n";
}
void print(char const* c)
{
    cout << " Here is char* " << c << "\n";
}
int main()
{
    print(10);
    print(10.10);
    print("ten");
  
    return 0;
}
