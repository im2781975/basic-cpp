//Copy of vector by insert() function 
#include<iostream> 
#include<vector> 
using namespace std; 
  
int main() 
{
    vector<int> vect1{1, 2, 3, 4}; 
  
    vector<int> vect2; 
 
    vect2.insert(vect2.begin(), vect1.begin(), vect1.end()); 
  
    cout<<"Vector elements are : ";
    for (int i=0; i<vect1.size(); i++) 
        cout << vect1[i] << " "; 
        
    cout<<"\nNew vector elements are : "; 
    for (int i=0; i<vect2.size(); i++) 
        cout << vect2[i] << " "; 
    
    vect1[0] = 2; 
  
    cout<<"\nFirst element of old vector is :"<<vect1[0];
    
    cout << "\nThe first element of new vector is :"<<vect2[0];
  
    return 0; 
} 
