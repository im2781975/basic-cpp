#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;

    vector<int> v;

    for(int i = 0; i < n; i++)
    {
        int input;
        cin >> input;
        v.push_back(input);
    }

    int sz = v.size();
    cout<<"\nElement of vector are: ";
    for(int i = 0; i < sz; i++)
    {
        cout << v[i] << " ";
    }

    int first_element = v.front();
    cout << "\nFirst element of vect are: "<<first_element;

    int last_element = v.back();
    cout << "\nLast element of vect are: " <<last_element ;

    cout<<"\nAfter erasing last element vect are: ";
    v.pop_back();
    sz = v.size();
    for(int i = 0; i < sz; i++)
        cout << v[i] << " ";

    cout <<"\nSize of the vector are: " <<sz ;
    
    cout<<"\nWith using loop & pop elements are: ";
    while(!v.empty())
    {
        cout << v.back() << " ";
        v.pop_back();
    }

    // Resize the vector and display elements
    cout<<"\nAfter resize elements are: ";
    v.resize(10, 1);
    for(int i = 0; i < v.size(); i++)
    {
        cout << v[i] << " ";
    }

    // Clear the vector
    v.clear();
    sz = v.size();
    cout<< "\nAfter clear sz is: "<<sz ;

    // Check if the vector is empty
    int emp = v.empty();
    cout << "\nis empty: "<<emp ;
}
