//copy of vector by assign() and copy().  
#include<iostream>  
#include<vector> 
#include<algorithm> 
#include<iterator> 
using namespace std;  
int main()  
{  
    vector<int> vect1{1, 2, 3, 4};
    vector<int> vect2;  
  
    copy(vect1.begin(), vect1.end(), back_inserter(vect2));  
    cout <<"Elements of vect1[] are: ";  
    for (int i=0; i<vect1.size(); i++)  
        cout << vect1[i] << " ";  
  
    cout << "\nAfter copy elements of vect2[] are : ";
    for (int i=0; i<vect2.size(); i++)  
        cout << vect2[i] << " ";  
  
    vect1[0] = 2;  
  
    cout << "\nFirst element of vect1[] is:" <<vect1[0];
    cout << "\nFirst element of vect2[] is:" <<vect2[0];
    
    return 0;  
}  
