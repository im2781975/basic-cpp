#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
    vector<int> vec{ 4, 2, 7, 9, 3, 5, 10, 6 }; 

    sort(vec.begin(), vec.end(), not2(greater<int>())); 
    cout<<"Ascending sort using not2:";
    for (int i = 0; i < vec.size(); i++) 
        cout << vec[i] << " "; 
    
    sort(vec.begin(), vec.end(), not2(less<int>())); 
    
    cout<<"\nDescending sort using not2:";
    for (int i = 0; i < vec.size(); i++) 
        cout << vec[i] << " "; 
    
    return 0;
} 
