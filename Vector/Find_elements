#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    vector<int> v;
 
    for (int i = 1; i <= 10; i++)
        v.push_back(i * 10);
 
    cout <<"Elements of v[2] :" <<v[2]<<"\n";
 
    cout << "Elements of v.at(4) : " << v.at(4)<<"\n";
 
    cout << "Elements of v.front() : " << v.front()<< "\n";
 
    cout << "Elements of v.back() : " << v.back();
 
    // pointer to the first element
    int* pos = v.data();
 
    cout << "\nThe first element is " << *pos;
    return 0;
}
