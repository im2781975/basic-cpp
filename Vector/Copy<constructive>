//copy of vectorby constructor method.
#include<bits/stdc++.h>  
using namespace std;  
  
int main()  
{  
    vector<int> vect1{1, 2, 3, 4};  
    //constructor method, Deep copy  
    vector<int> vect2(vect1);  
  
    cout << "Before copy vect1[] is: ";  
    for (int i=0; i<vect1.size(); i++)  
        cout << vect1[i] << " ";  
  
    cout << "\nAfter copy vect2[] is : ";  
    for (int i=0; i<vect2.size(); i++)  
        cout << vect2[i] << " ";  

    vect1[0] = 2;  
  
    cout << "\nAfter insert first element of vect1[] is : "<< vect2[0];
    cout << "\nFirst element of vect2[] is : "<<vect2[0];
    
    return 0;  
}  
