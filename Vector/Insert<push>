#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n; cin >>n;
    vector <int>a;
    
    for(int i=0;i<n;i++)
        a.push_back(i);
    
    cout<<"After insertion: ";
    a.insert(a.begin()+1,100);
    for(int i=0;i<a.size();i++)
    {
        cout<<" "<<a[i];
    }
    
    cout<<"\nAfter delete: ";
    a.erase(a.begin()+2);
    
    for(int i=0;i<a.size();i++)
    {
        cout<<" "<<a[i];
    }
    
}
