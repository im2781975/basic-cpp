#include<bits/stdc++.h>
using namespace std;
//integral means integers.
template <typename T
concept C = is_integral_v<T>;

// Function template with constrained auto
auto getsum(C auto value1, C auto value2)
{
    return value1 + value2;
}
int main()
{
    int result = getsum(3, 6);

    cout << "The sum is: " << result <<"\n";

    return 0;
}

