#include <iostream>
#include <type_traits> // Include the type_traits header for is_integral_v

using namespace std;

// Defining concept
template <typename T>
concept C = is_integral_v<T>;

// Function template with constrained auto
auto getsum(C auto value1, C auto value2)
{
    return value1 + value2;
}

int main()
{
    // Using integers for the constrained auto parameters
    int result = getsum(3, 6);

    cout << "The sum is: " << result << endl;

    return 0;
}

