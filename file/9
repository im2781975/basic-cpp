#include<iostream>
#include<fstream>
using namespace std;
int main()
{
    //of->print to a file
    //ifs->take a input from a file
    ofstream of1;
    of1.open("1.txt");
    of1<<"ibrahim molla"<<endl;
    ifstream ifs;
    ifs.open("2.open");
    int x;
    double y,z;
    ifs>>x>>y>>z;
    cout<<x<<" "<<y<<" "<<z<<endl;
    of1.close();
    ifs.close();
}
