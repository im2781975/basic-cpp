#include <iostream>
using namespace std;
 
class Abstract {
    private:
    int a, b;
    public:
    //Abstract(int x,int y):a(x),b(y){}
    void set(int x, int y)
    {
        a = x;
        b = y;
    }
 
    void display()
    {
        cout << "a = " << a << " b = " << b << "\n";
    }
};
 
int main()
{
    //Abstract obj(10,20);
    Abstract obj;
    obj.set(10, 20);
    obj.display();
    return 0;
}
