#include <iostream>
using namespace std;
 
class Encapsulation {
private:
    // we declare a as private to hide it from outside
    int a;

    public:
    void set(int x)
    {
        a = x;
    }

    int get()
    {
        return a;
    }
};

int main()
{
    Encapsulation e1;
 
    e1.set(10);
    cout<<e1.get();
    return 0;
}
