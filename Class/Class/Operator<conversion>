//working of conversion operator
#include<bits/stdc++.h>
using namespace std;
class fraction{
    int num, digit;
    public:
    fraction(int num, int digit){
        this->num = num;
        this->digit = digit;
    }
    operator float(){
        return float(num)/float(digit);
    }
};
int main(){
    fraction f(9, 11);
    cout << f;
}
