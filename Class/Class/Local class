#include <iostream>
using namespace std;
  
void fun()
{
    class Test 
    {
    public:
    
        static void method()
        {
            cout << "\nLocal Class method() called";
        }
    };
    cout<<"Yes";
    //Test ::method();
    Test t;
    t.method();
}
  
int main()
{
    fun();
    return 0;
}
