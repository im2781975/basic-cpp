#include <bits/stdc++.h>
using namespace std;
class Dist{
    float feet, inch;
    public:
    Dist(float inch, float feet){
        this ->inch = inch;
        this->feet = feet;
    }
    // Overloading(-) operator to perform decrement operation of Distance object
    void operator -(){
        feet--; inch--;
        cout << feet << " " << inch << "\n";
    }
    void operator +(){
        feet++; inch++;
        cout << feet << " " << inch << "\n";
    }
};
int main(){
    Dist d(8, 9);
    -d;
    +d;
}
