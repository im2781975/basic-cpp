#include<bits/stdc++.h>
using namespace std;
class student
{
    public:
    string name;
    string father_name;
    string mother_name;
    int std_id;
    int age;
    student(string s,string father,string mother,int id,int ag)
    {
        name=s;
        father_name=father;
        mother_name=mother;
        std_id=id;
        age=ag;
    }
    student(string name,string father,string mother)
    {
        this->name=name;
        father_name=father;
        mother_name=mother;
    }
    void print()
    {
        cout<<name<<" "<<father_name<<" "<<mother_name<<" "<<std_id<<" "<<age<<"\n";
    }
};
int main()
{
    student s("A","B","C",11026,26);
    s.print();
    student s2("X","Y","Z");
    s2.print();
}
