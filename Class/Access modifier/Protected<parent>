#include <bits/stdc++.h>
using namespace std;
class parent{
    protected:
    int x;
};
class child:public parent{
    public:
    //Child class is able to access the inherited protected
    //data members of base class
    void set(int y){
        x = y;
    }
    void show(){
        cout << x << " ";
    }
};
int main(){
    child c;
    c.set(89);
    c.show();
}
