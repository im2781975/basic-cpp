#include <bits/stdc++.h>
using namespace std;
class student{
    int roll;
    string name;
    double fee;
    public:
    student(int roll, string name, double fee);
    void display();
};
student::student(int roll, string name, double fee){
    this->roll = roll;
    this->name = name;
    this->fee = fee;
}
void student::display(){
    cout << "Name: " << name << "\nroll: " << roll << "\nfee: " << fee << "\n";
}
int main(){
    student stud(1000, "ibrahim", 2340);
    stud.display();
}
