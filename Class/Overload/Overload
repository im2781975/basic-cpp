// operators that can be overloaded
#include <bits/stdc++.h>
using namespace std;
class base{
    public:
    int num;
    base(int i):num(i){}
    base *operator->(){
        return this;
    }
};
int main(){
    base b(5);
    cout << "b.num: " << b.num << "\n";
    base *ptr = &b;
    cout << "ptr->num: " << ptr->num << "\n";
    // Accessing num using -> operator
    cout << b->num;
}
