#include <iostream>
using namespace std;

class Base {
public:
    virtual ~Base() {
        cout << "~Base() is executed" << endl;
    }
};

class Derived : public Base {
public:
    ~Derived() {
        cout << "~Derived() is executed" << endl;
    }
};

int main() {
    
    Base* b = new Derived();
    delete b;
    return 0;
}

