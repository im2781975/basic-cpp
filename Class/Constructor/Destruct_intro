#include <bits/stdc++.h>
using namespace std;
class Geeks
{
    public:
    int id;
    
    Geeks()
    {
        cout <<"Default Constructor called" <<"\n";
        id=-1;
    }
    
    Geeks(int x)
    {
        cout<<"Parameterized Constructor called "<<"\n";
        id=x;
    }
    ~Geeks()
    {
        cout<<"Destructor called here-"<<id<<"\n";
    }
};
int main()
{
    Geeks obj1;
    cout <<"Geek id is: "<<obj1.id <<"\n";

    Geeks obj2(21);
    cout <<"Geek id is: " <<obj2.id <<"\n";
    Geeks obj3(7); 
    
    int i = 0;
    while ( i < 5 )
    {
        Geeks obj4(i); 
        i++;        
    } 
    return 0;
}
