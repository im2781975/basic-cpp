#include <iostream>
using namespace std;
class Demo{
   int a, b;
    public:
    Demo()   // default constructor
    {
        cout<<"Default Constructor" <<"\n";
    }
    Demo(int a, int b):a(a),b(b)
    {
        cout << "parameterized constructor values :" << a << " "<< b <<"\n";
    }
}instance(100,200);
//instance;
int main() {
     
    return 0;
}
