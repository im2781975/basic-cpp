#include <iostream>
using namespace std;

class A {
public:
    int sum = 0;
    A(); 
//default constructor with no arg
    A(int a, int x = 0)
// default constructor with one no arg
    {
        sum = a + x;
    }
    void print()
    { cout <<"Sum ="<<sum <<"\n"; }
};
int main()
{
    A obj1(10, 20);
    A obj2(5);
    obj1.print();
    obj2.print();
    return 0;
}
