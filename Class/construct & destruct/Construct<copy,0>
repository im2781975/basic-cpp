#include<iostream>
using namespace std;
 
class Sample
{
    int id;
    public:
    void init(int x)
    {
        id=x;   
    }   
    Sample(){}
    
    //copy constructor
    Sample(Sample &t)  
    {
        id=t.id;
    }
    void display()
    {
        cout<<"\n"<<"ID="<<id;
    }
};
int main()
{
    Sample obj1;
    obj1.init(10);
    obj1.display();
    Sample obj2(obj1);    
    //copy constructor called
    obj2.display();
    return 0;
}
