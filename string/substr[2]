#include <bits/stdc++.h>
using namespace std;
void subString(string s, int n)
{
    for (int i = 0; i < n; i++)
        for (int len =1; len <= n - i; len++)
            cout << s.substr(i, len) <<"\n";
}
void subString1(string s, int n)
{
      vector<int> v;
    for (int i = 0; i < n; i++){
        for (int len = 1; len <= n - i; len++){
             string sub =(s.substr(i, len));
             int x=stoi(sub);
             v.push_back(x);
            }
     }
     cout<<*max_element(v.begin(),v.end())<<endl;
}
int main()
{
    //string s = "abcd";
    string s="823";
    subString1(s, s.length());
    return 0;
}
