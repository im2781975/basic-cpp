// C++ program to handle cases when we
// don't know how many lines of input 
// are going to be there
#include <iostream
using namespace std;
int main()
{
    string s;
    cout<<"Enter string: ";
    while (getline(cin, s))
    {
        if (s.empty())
            break;
        cout << s << "\n";
    }
    return 0;
}
