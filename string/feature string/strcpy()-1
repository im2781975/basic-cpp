#include <iostream>
#include <cstring>
using namespace std;
 
int main()
{
    char src[] = "geeksforgeeks";
 
    // Here destination is large enough
    // to store the src with Null
    // character at the end
    char dest[14];
 
    // copying src into dest.
    strcpy(dest, src);
    cout << "Copied string: " << dest << endl;
     
    return 0;
}
