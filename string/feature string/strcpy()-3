#include <iostream>
#include <cstring>
using namespace std;
 
int main()
{
    char src[] = "geeksforgeeks";
     
    // The destination string size is 14.
    char dest[14];
     
    // copying n bytes of src into dest.
    strncpy(dest, src, 14);
    cout << "Copied string: " << dest << endl;
     
    return 0;
}
