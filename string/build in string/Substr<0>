#include <bits/stdc++.h>
using namespace std;
void subString(string s, int n)
{//Pick starting point in outer loop & lengths of different strings for a given starting point
    for (int i = 0; i < n; i++)
        for (int len = 1; len <= n - i; len++)
            cout << s.substr(i, len) <<"\n";
}
int main()
{
    string s = "abcd";
    subString(s, s.length());
    return 0;
}
