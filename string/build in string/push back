#include<bits/stdc++.h>
using namespace std;
void push_backDemo(string str1, string str2)
{
    for(int i = 0; str2[i] != '\0'; i++)
    {
        str1.push_back(str2[i]);
    }
    cout << "After push_back : ";
    cout << str1;
}
int main()
{
    string str1("Geeksfor");
    string str2("Geeks");
 
    cout << "Original String : " << str1 << endl;
    push_backDemo(str1, str2);
 
    return 0;
}
