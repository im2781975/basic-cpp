#include<bits/stdc++.h>
using namespace std;
int main()
{
    char str1[10] = "aksh";
    char str2[10] = "akash";
 
    int res =strncmp(str1,str2,4);
 
    if (res == 0)
    {
    cout<<"str1 is equal to str2 up to numb characters"<<"\n";
    }
    else if (res > 0)
        cout << "str1 is greater than str2" << "\n";
    else
        cout << "str2 is greater than str1" <<"\n";
 
    cout << "Value returned by strncmp() is: " << res
         << "\n";
//---------------------------------
    char* st1 = "GeeksforGeeks";
    char* st2 = "Geeks For Geeks";
    int value = strncmp(st1, st2, 5);
    if (!value) 
    {
        cout << "The first 5 characters are same.";
    }
    else {
        cout << "The first 5 characters are not equal.";
    }
    return 0;
}
