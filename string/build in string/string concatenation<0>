#include<bits/stdc++.h>
using namespace std;
int main()
{
    string init("this is init");
    string add(" added now");
    //init.append(add);
   // init = init + add;
    //cout << init <<"\n";

     string output;
    for (int i = 0; init[i] != '\0'; i++)
    {
        output += init[i];
    }
    for (int i = 0; add[i] != '\0'; i++)
    {
        output += add[i];
    }
 
    cout << output << endl;
    return 0;
}
