/*can compare
x is null */
#include <bits/stdc++.h>
using namespace std;
int main()
{
    nullptr_t np1, np2;
    if (np1 == np2)
        cout << "can compare" << endl;
    else
        cout << "can not compare" << endl;
    char *x = np1;  
    
    if (x == nullptr)
        cout << "x is null" << endl;
    else
        cout << "x is not null" << endl;
 
    return 0;
}
